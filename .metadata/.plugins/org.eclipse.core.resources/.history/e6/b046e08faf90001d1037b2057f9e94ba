
public class Car {
	//필드 선언
	String company = "현대자동차";
	String model = "자가용";
	String color = "은색";
	int maxSpeed = 250;
	
	Car(){
	}
	
	Car(String model){
		this.model = model;
	}
	
	Car(String model, String color){
		this.model = model;
		this.color = color;
	}
	
	Car(String model, String color, int maxSpeed){
		this.model = model;
		this.color = color;
		this.maxSpeed = maxSpeed;
	}
}
