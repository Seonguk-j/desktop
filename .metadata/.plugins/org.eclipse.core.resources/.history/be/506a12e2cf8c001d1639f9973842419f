
public class Exercise3_7 {

	public static void main(String[] args) {
		int fahrenheit = 100;
		float celcius = (float) (((float)5 / 9 * (fahrenheit - 32) * 1000) % 10 < 5 ? (float)((int)((float)5 / 9 * (fahrenheit - 32) * 100)) / 100 : (float)((int)((float)5 / 9 * (fahrenheit - 32) * 100)) / 100 + 0.01);
		
		System.out.println("Fahrenheit : " + fahrenheit);
		System.out.println("Celcius : " + celcius);
	}

}
