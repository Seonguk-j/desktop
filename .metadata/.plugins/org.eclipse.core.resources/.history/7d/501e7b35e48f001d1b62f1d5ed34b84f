
public class quiz_04 {

	public static void main(String[] args) {
		String[][] arr1 = new String[3][3];
		
		int[][] arr2 = new int[4][4];
		int cnt = 0;
		for(int i = 0; i < arr2.length; i++) {
			for(int j = 0; j < arr2[i].length; j++) {
				arr2[i][j] = ++cnt;
				System.out.println(arr2[i][j] + " ");
			}
			System.out.println();
		}
	}

}
